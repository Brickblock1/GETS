							0: 3;
							1: 3;
							2: 3;}

					switch(FEAT_TRAINS, SELF, switch_FX_V_140, position_in_vehid_chain % 3) {
						0: return visual_effect_and_powered(VISUAL_EFFECT_DIESEL, -1, DISABLE_WAGON_POWER);
						1: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, -1, DISABLE_WAGON_POWER);
							return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);}
						
					switch(FEAT_TRAINS,PARENT,switch_running_cost_V_140, 1){
						return 1625;}
		
					switch (FEAT_TRAINS,SELF, switch_V_140_build_year,build_year){
						    0 .. 1945: switch_spritestack_V_140_DRG;
						 1946 .. 2050: switch_spritestack_V_140_DB;							 					 
