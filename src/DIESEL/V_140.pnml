						0:	   spriteset_empty;
						1:     return switch_V_140_build_year;
						2:	   spriteset_empty;}	

					switch (FEAT_TRAINS, SELF, switch_articulated_V_140_build, extra_callback_info1) {
						1: return ITEM_V_140_BUILD; 
						2: return ITEM_V_140_BUILD;
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}

item (FEAT_TRAINS , ITEM_V_140_BUILD, 2396)
		{
		property 
			{
			//common properties
						name: 							string(STR_NAME_V_140_BUILD);
						extra_flags:					bitmask(VEHICLE_FLAG_SYNC_VARIANT_EXCLUSIVE_PREVIEW);
						climates_available: 			ALL_CLIMATES;
						introduction_date: 				date(1935,01,01);
						model_life: 					18;
						vehicle_life: 					18;
						retire_early: 					-10;	
						reliability_decay: 				20;
						cargo_allow_refit: 				[PASS] ;
						cargo_capacity: 				1 ;
						cost_factor: 					108;
						misc_flags: 					bitmask(TRAIN_FLAG_SPRITE_STACK);	
						running_cost_base: 				RUNNING_COST_STEAM;
						running_cost_factor: 			90;
						
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							100 km/h;
						refit_cost: 					0;
						track_type: 					RAIL;
						power:  						1400 hp;
						weight: 						83 ton;
						ai_engine_rank: 				0;
						engine_class: 					ENGINE_CLASS_DIESEL;
						visual_effect_and_powered: 		visual_effect_and_powered(VISUAL_EFFECT_DIESEL, 2, DISABLE_WAGON_POWER);
						dual_headed: 					0;
						tractive_effort_coefficient: 	0.168;
						length: 						8;										
			}

		graphics
			{
			default							:switch_V_140_graphics_build;
			additional_text				    :string(STR_CONCAT_2, 
											 string(STR_TRACTION_DIESELHYDRAULIC), 
											 string(STR_DESC_UNIVERSAL),);
			length							:switch_length_V_140;
			articulated_part				:switch_articulated_V_140_build;
			purchase_cargo_capacity        	:0 ;
			cost_factor						:40;
			cargo_capacity                 	:return 0 ;
			purchase					   	:switch_V_140_purchase;
			running_cost_factor				:switch_running_cost_V_140;
			visual_effect_and_powered		:switch_FX_V_140;
			
			}
	}

					switch (FEAT_TRAINS,SELF, switch_V_140_current_year,current_year){
						    0 .. 1945: switch_spritestack_V_140_DRG;
						 1946 .. 2050: switch_spritestack_V_140_DB;							 					 
						 switch_spritestack_V_140_DB;}

					switch(FEAT_TRAINS, SELF, switch_V_140_graphics_current, position_in_vehid_chain % 3) {
						0:	   spriteset_empty;
						1:     return switch_V_140_current_year;
						2:	   spriteset_empty;}	

