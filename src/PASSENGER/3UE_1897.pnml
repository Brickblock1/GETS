spriteset(spriteset_3UE_1897_KPEV_purchase, "gfx\3UE_1897/3UE_1897_KPEV_AB_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_3UE_1897_KPEV_purchase, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx\3UE_1897/3UE_1897_KPEV_AB_1x_32bpp.png", "gfx\3UE_1897/3UE_1897_KPEV_AB_1x_mask.png")
