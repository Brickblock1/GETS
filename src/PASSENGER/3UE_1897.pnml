spriteset(spriteset_3UE_1897_DRG_purchase, "gfx\3UE_1897/3UE_1897_DRG_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_3UE_1897_DRG_purchase, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx\3UE_1897/3UE_1897_DRG_1x_32bpp.png", "gfx\3UE_1897/3UE_1897_DRG_1x_mask.png")
									{ template_purchase(0, 0)}	
									
									
//Switches

					switch (FEAT_TRAINS,SELF, switch_3UE_1897_purchase,current_year){
						    0 .. 1919: spriteset_3UE_1897_KPEV_purchase;
						 1920 .. 2050: spriteset_3UE_1897_DRG_purchase;						 
							spriteset_3UE_1897_DRG_purchase;}																	
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_3UE_1897_KPEV_AB, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_3UE_1897_KPEV_AB;
							1: spritegroup_3UE_1897_KPEV_AB_loading;}																									

						switch(FEAT_TRAINS, SELF, switch_spritestack_3UE_1897_KPEV_C, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_3UE_1897_KPEV_C;
							1: spritegroup_3UE_1897_KPEV_C_loading;}
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_3UE_1897_DRG, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_3UE_1897_DRG;
							1: spritegroup_3UE_1897_DRG_loading;}								

					switch (FEAT_TRAINS, SELF, random_choose_3UE_1897_KPEV, switch_alternative_var_random_bits(2, 2)) {
							1: switch_spritestack_3UE_1897_KPEV_AB;
							2: switch_spritestack_3UE_1897_KPEV_C;
							3:switch_spritestack_3UE_1897_KPEV_C;											
								switch_spritestack_3UE_1897_KPEV_C;}

						switch (FEAT_TRAINS, PARENT, switch_running_cost_3UE_1897,  switch_check_Restaurant_0()) {
							1: return 0;
							0: return 1025;}	
							
						switch (FEAT_TRAINS, PARENT, switch_cargo_aging_3UE_1897,  switch_check_Restaurant_0()) {
							1: return 232;
							0: return 185;}																
								
					switch (FEAT_TRAINS,SELF,switch_current_year_3UE_1897,current_year) {
							   0 .. 1919 : random_choose_3UE_1897_KPEV;
							1920 .. 2050 : switch_spritestack_3UE_1897_DRG;
								switch_spritestack_3UE_1897_DRG;}
						
					switch (FEAT_TRAINS,SELF,switch_build_year_3UE_1897,build_year) {
							   0 .. 1919 : random_choose_3UE_1897_KPEV;
							1920 .. 2050 : switch_spritestack_3UE_1897_DRG;
								switch_spritestack_3UE_1897_DRG;}
								
								
