		alternative_sprites (spriteset_4ue_PR05_purchase3, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx/KPEV_4ue_PR05/B4ue_PR05_DB_1x_32bpp.png", "gfx/KPEV_4ue_PR05/B4ue_PR05_DB_1x_mask.png")
					{template_purchase(0, 0)}					
					
//Switches					

					switch (FEAT_TRAINS,SELF,switch_purchase_4ue_PR05,current_year) {
						   0 .. 1919 : spriteset_4ue_PR05_purchase1;
						1920 .. 1945 : spriteset_4ue_PR05_purchase2;
						1946 .. 2050 : spriteset_4ue_PR05_purchase3;
							spriteset_4ue_PR05_purchase3;}

						switch(FEAT_TRAINS, SELF, switch_spritestack_4ue_PR05_AB_KPEV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_4ue_PR05_AB_KPEV;
							1: spritegroup_4ue_PR05_AB_KPEV_loading;}
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_4ue_PR05_ABC_KPEV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_4ue_PR05_ABC_KPEV;
							1: spritegroup_4ue_PR05_ABC_KPEV_loading;}
							
