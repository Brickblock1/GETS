//Switches					

					switch (FEAT_TRAINS,SELF,switch_purchase_Post_1891,current_year) {
						   0 .. 1919 : spriteset_Post_1891_purchase1;
						1920 .. 1945 : spriteset_Post_1891_purchase2;
						1946 .. 2050 : spriteset_Post_1891_purchase3;
							spriteset_Post_1891_purchase3;}
						
					switch (FEAT_TRAINS,SELF,switch_build_year_Post_1891,build_year) {
