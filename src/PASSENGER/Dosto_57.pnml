//Spriteset Purchase

spriteset (spriteset_DOSTO_57_purchase1, "gfx\DOSTO_57/Dosto_57_blau_1x_8bpp.png")
					{template_purchase(0, 0)}
				
		alternative_sprites (spriteset_DOSTO_57_purchase1, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx\DOSTO_57/Dosto_57_blau_1x_32bpp.png", "gfx\DOSTO_57/Dosto_57_blau_1x_mask.png")
