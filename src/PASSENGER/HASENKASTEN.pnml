spritegroup spritegroup_Hasenkasten_loading
		{loaded: 	[spriteset_empty];
			loading: 	[spriteset_Hasenkasten_loading];}	
												
//Spritesets purchase									

spriteset(spriteset_Hasenkasten_purchase_schwarz, "gfx\N_WAGEN_DVT/Hasenkasten_schwarz_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_Hasenkasten_purchase_schwarz, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx\N_WAGEN_DVT/Hasenkasten_schwarz_1x_32bpp.png", "gfx\N_WAGEN_DVT/Hasenkasten_schwarz_1x_mask.png")
									{ template_purchase(0, 0)}
									
spriteset(spriteset_Hasenkasten_purchase_ozeanblau, "gfx\N_WAGEN_DVT/Hasenkasten_ozeanblau_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_Hasenkasten_purchase_ozeanblau, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx\N_WAGEN_DVT/Hasenkasten_ozeanblau_1x_32bpp.png", "gfx\N_WAGEN_DVT/Hasenkasten_ozeanblau_1x_mask.png")
									{ template_purchase(0, 0)}	
									
//Switches

						switch (FEAT_TRAINS,SELF, switch_purchase_Hasenkasten,current_year){
							   0 .. 1974: spriteset_Hasenkasten_purchase_schwarz;
							1975 .. 2050: spriteset_Hasenkasten_purchase_ozeanblau;
								spriteset_Hasenkasten_purchase_ozeanblau;}

						switch(FEAT_TRAINS, SELF, switch_spritestack_Hasenkasten_schwarz, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Hasenkasten_schwarz;
							1: spritegroup_Hasenkasten_loading;}																		

						switch(FEAT_TRAINS, SELF, switch_spritestack_Hasenkasten_schwarz_ohne, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Hasenkasten_schwarz_ohne;
							1: spritegroup_Hasenkasten_loading;}

						switch(FEAT_TRAINS, SELF, switch_spritestack_Hasenkasten_ozeanblau, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Hasenkasten_ozeanblau;
							1: spritegroup_Hasenkasten_loading;}																		

						switch(FEAT_TRAINS, SELF, switch_spritestack_Hasenkasten_ozeanblau_ohne, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Hasenkasten_ozeanblau_ohne;
							1: spritegroup_Hasenkasten_loading;}							
							   
						switch (FEAT_TRAINS, SELF, random_choose_Hasenkasten_schwarz, switch_alternative_var_random_bits(1, 1)) {
							1: switch_spritestack_Hasenkasten_schwarz;
								switch_spritestack_Hasenkasten_schwarz_ohne;}							

						switch (FEAT_TRAINS, SELF, random_choose_Hasenkasten_ozeanblau, switch_alternative_var_random_bits(1, 1)) {
							1: switch_spritestack_Hasenkasten_ozeanblau;
								switch_spritestack_Hasenkasten_ozeanblau_ohne;}	
								
						switch(FEAT_TRAINS, SELF, switch_length_Hasenkasten, position_in_vehid_chain % 3) {
							0: 4;
							1: 7;
							2: 4;}										

						switch(FEAT_TRAINS, SELF, switch_speed_Hasenkasten, current_year){
							   0 .. 1967: 120;
							1968 .. 2050: 140;
								140;}

						switch(FEAT_TRAINS,PARENT,switch_running_cost_Hasenkasten, 1){
							return 2350;}	

						switch (FEAT_TRAINS, SELF, switch_build_year_Hasenkasten, build_year){
							   0 .. 1974 : random_choose_Hasenkasten_schwarz;
							1975 .. 2050 : random_choose_Hasenkasten_ozeanblau;
								random_choose_Hasenkasten_ozeanblau;}

						switch(FEAT_TRAINS, SELF, switch_graphics_Hasenkasten_build, position_in_vehid_chain  % 3) {
								0:     spriteset_empty;
								1:     return switch_build_year_Hasenkasten;
								2:     spriteset_empty;}

						switch (FEAT_TRAINS, SELF, switch_articulated_Hasenkasten_build, extra_callback_info1) {
							1 .. 2: return ITEM_HASENKASTEN_BUILD;
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}

