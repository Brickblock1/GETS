							2:     spriteset_empty;}	

					switch (FEAT_TRAINS, SELF, switch_articulated_Pw4ue_30_build, extra_callback_info1) {
						1 .. 2: return ITEM_PW4UE_30_BUILD;											
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}
																			
item (FEAT_TRAINS , ITEM_PW4UE_30_BUILD, 5886)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_PW4UE_30_BUILD);
