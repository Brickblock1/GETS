			additional_text					: string(STR_DESC_BAGG_EXP);
			length							: switch_length_Pw4ue_30;
			articulated_part				: switch_articulated_Pw4ue_30_build;
			purchase						: switch_Pw4ue_30_purchase;
			cost_factor						: 485;
			running_cost_factor				: switch_running_cost_Pw4ue_30;				
