spriteset(spriteset_Coach_1891_DRG_purchase, "gfx\Coach_1891/Coach_1891_DRG_D_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_Coach_1891_DRG_purchase, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx\Coach_1891/Coach_1891_DRG_D_1x_32bpp.png", "gfx\Coach_1891/Coach_1891_DRG_D_1x_mask.png")
									{ template_purchase(0, 0)}
									
//Switches

					switch (FEAT_TRAINS,SELF, switch_Coach_1891_purchase,current_year){
						    0 .. 1919: spriteset_Coach_1891_KPEV_purchase;
						 1920 .. 2050: spriteset_Coach_1891_DRG_purchase;						 
							spriteset_Coach_1891_DRG_purchase;}
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1891_KPEV_AB, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1891_KPEV_AB;
							1: spritegroup_Coach_1891_loading;}																									

						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1891_KPEV_C, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1891_KPEV_C;
							1: spritegroup_Coach_1891_loading;}
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1891_KPEV_CD, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1891_KPEV_CD;
							1: spritegroup_Coach_1891_loading;}
														
						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1891_KPEV_D, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1891_KPEV_D;
							1: spritegroup_Coach_1891_loading;}		
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1891_DRG_AB, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1891_DRG_AB;
							1: spritegroup_Coach_1891_loading;}																									
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1891_DRG_D, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1891_DRG_D;
							1: spritegroup_Coach_1891_loading;}																		

					switch (FEAT_TRAINS, SELF, random_choose_Coach_1891_KPEV, switch_alternative_var_random_bits(1, 3)) {
							1: switch_spritestack_Coach_1891_KPEV_AB;
							2: switch_spritestack_Coach_1891_KPEV_C;
							3: switch_spritestack_Coach_1891_KPEV_C;
							4: switch_spritestack_Coach_1891_KPEV_CD;
							5: switch_spritestack_Coach_1891_KPEV_CD;
							6: switch_spritestack_Coach_1891_KPEV_D;
							7: switch_spritestack_Coach_1891_KPEV_D;																										
								switch_spritestack_Coach_1891_KPEV_D;}

					switch (FEAT_TRAINS, SELF, random_choose_Coach_1891_DRG, switch_alternative_var_random_bits(1, 1)) {
							1: switch_spritestack_Coach_1891_DRG_AB;									
								switch_spritestack_Coach_1891_DRG_D;}
								
					switch(FEAT_TRAINS,PARENT,switch_running_cost_Coach_1891, 1){
						return 850;}								
								
					switch (FEAT_TRAINS,SELF,switch_current_year_Coach_1891,current_year) {
							   0 .. 1919 : random_choose_Coach_1891_KPEV;
							1920 .. 2050 : random_choose_Coach_1891_DRG;
								random_choose_Coach_1891_DRG;}
						
					switch (FEAT_TRAINS,SELF,switch_build_year_Coach_1891,build_year) {
							   0 .. 1919 : random_choose_Coach_1891_KPEV;
							1920 .. 2050 : random_choose_Coach_1891_DRG;
								random_choose_Coach_1891_DRG;}			
								
