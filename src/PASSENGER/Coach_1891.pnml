		{	loaded: 	[spriteset_empty];
			loading: 	[spriteset_Coach_1891_loading];}										

//Spritesets purchase

spriteset(spriteset_Coach_1891_KPEV_purchase, "gfx\Coach_1891/Coach_1891_KPEV_C_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_Coach_1891_KPEV_purchase, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx\Coach_1891/Coach_1891_KPEV_C_1x_32bpp.png", "gfx\Coach_1891/Coach_1891_KPEV_C_1x_mask.png")
