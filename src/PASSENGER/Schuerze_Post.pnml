						switch (FEAT_TRAINS, SELF, switch_articulated_Schuerze_Post_build, extra_callback_info1) {
							1 .. 2: return ITEM_SCHUERZE_POST_BUILD;												
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}
																			
