							0: spriteset_YL_AB_ozeanblau;
							1: spritegroup_YL_ozeanblau_loading;}		
							
						switch (FEAT_TRAINS, SELF, random_choose_YL_gruen, switch_alternative_var_random_bits(1, 2)) {
							1: switch_loadstack_YL_B_gruen;
							2: switch_loadstack_YL_B_gruen;
							3: switch_loadstack_YL_B_gruen;
							   switch_loadstack_YL_AB_gruen;}	
							   
						switch (FEAT_TRAINS, SELF, random_choose_YL_ozeanblau, switch_alternative_var_random_bits(1, 2)) {
							1: switch_loadstack_YL_B_ozeanblau;
							2: switch_loadstack_YL_B_ozeanblau;
							3: switch_loadstack_YL_B_ozeanblau;
							   switch_loadstack_YL_AB_ozeanblau;}								   
							   						   																																
						switch(FEAT_TRAINS, SELF, switch_length_YL, position_in_articulated_veh % 3) {
							0: 4;
							1: 7;
							2: 4;}

						switch(FEAT_TRAINS,PARENT,switch_running_cost_YL, 1){
							return 2050;}
							
						switch (FEAT_TRAINS,SELF,switch_build_year_YL,build_year) {
							  00 .. 1974 : random_choose_YL_gruen;
							1975 .. 2050 : random_choose_YL_ozeanblau;
								random_choose_YL_ozeanblau;}							

						switch(FEAT_TRAINS, SELF, switch_YL_graphics_build, position_in_articulated_veh  % 3) {
							0:     spriteset_empty;
							1:     return switch_build_year_YL;
							2:     spriteset_empty;}	
								
						switch (FEAT_TRAINS, SELF, switch_articulated_YL_build, extra_callback_info1) {
							1 .. 2: return ITEM_YL_BUILD;
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}


item (FEAT_TRAINS , ITEM_YL_BUILD, 7350)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_YL_BUILD);
						extra_flags:					bitmask(VEHICLE_FLAG_SYNC_VARIANT_EXCLUSIVE_PREVIEW);
						climates_available: 			ALL_CLIMATES;
						introduction_date: 				date(1951,01,01);
						model_life: 					40;
						vehicle_life: 					40;
						retire_early:					-10;						
						reliability_decay: 				20;
						misc_flags: 					bitmask(TRAIN_FLAG_SPRITE_STACK);	
						refittable_cargo_classes:		bitmask(CC_PASSENGERS);
						cargo_allow_refit:				[PASS, TOUR];
						loading_speed:					10;
						cost_factor:                    108;
						running_cost_base:              RUNNING_COST_DIESEL;
						running_cost_factor: 			90;
						
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							120 km/h;
						refit_cost: 					0;
						track_type: 					RAIL;						
						weight: 						33 ton;
						dual_headed: 					0;
						default_cargo_type:				PASS;
						cargo_capacity:					28;
						ai_engine_rank: 				0;
						length: 						8;
												
			}
			
	//Vehicels Graphics
			graphics
			{
			default							: switch_YL_graphics_build;
			additional_text					: string(STR_DESC_PASS);
			length							: switch_length_YL;
			articulated_part				: switch_articulated_YL_build;
			purchase						: switch_purchase_YL;
			cost_factor						: 310;
			running_cost_factor				: switch_running_cost_YL;					
			 }
		}

						switch (FEAT_TRAINS,SELF,switch_current_year_YL,current_year) {
							  00 .. 1974 : random_choose_YL_gruen;
							1975 .. 2050 : random_choose_YL_ozeanblau;
