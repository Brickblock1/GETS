									{ template_purchase(0, 0)}
									
//Switches

						switch (FEAT_TRAINS,SELF,switch_purchase_YL,current_year) {
							  00 .. 1974 : spriteset_YL_gruen_purchase;
							1975 .. 2050 : spriteset_YL_ozeanblau_purchase;
								spriteset_YL_ozeanblau_purchase;}	
								
						switch(FEAT_TRAINS, SELF, switch_loadstack_YL_B_gruen, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_YL_B_gruen;
							1: spritegroup_YL_gruen_loading;}	
							
						switch(FEAT_TRAINS, SELF, switch_loadstack_YL_AB_gruen, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_YL_AB_gruen;
							1: spritegroup_YL_gruen_loading;}	
							
						switch(FEAT_TRAINS, SELF, switch_loadstack_YL_B_ozeanblau, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_YL_B_ozeanblau;
							1: spritegroup_YL_ozeanblau_loading;}	
							
