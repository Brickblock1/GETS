item (FEAT_TRAINS , ITEM_PR99_PW_BUILD, 5604)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_PR99_PW_BUILD);
						extra_flags:					bitmask(VEHICLE_FLAG_SYNC_VARIANT_EXCLUSIVE_PREVIEW);
						climates_available: 			ALL_CLIMATES;
						introduction_date: 				date(1891,01,01);
						model_life: 					63;
						vehicle_life: 					40;
						retire_early:					-10;						
						reliability_decay: 				20;
						refittable_cargo_classes:		bitmask();
						cargo_allow_refit:				[MAIL, GOOD, VALU];
						loading_speed:					9;
						cost_factor:                    108;
						running_cost_base:              RUNNING_COST_DIESEL;
						running_cost_factor: 			90;
						
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							80 km/h;
						refit_cost: 					0;
						track_type: 					RAIL;						
						weight: 						16 ton;
						dual_headed: 					0;
						default_cargo_type:				MAIL;
						cargo_capacity:					92;
						ai_engine_rank: 				0;
						length: 						8;												
			}
			
	//Vehicels Graphics
			graphics
			{
			default							: switch_build_year_PR99_PW;
			additional_text					: string(STR_DESC_BAGG);
			purchase						: switch_purchase_PR99_PW;
			cost_factor						: 185;
			running_cost_factor				: switch_running_cost_PR99_PW;				
			 }
		}

