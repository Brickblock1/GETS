						switch (FEAT_TRAINS,SELF,switch_purchase_PW4ue,current_year) {
							  00 .. 1919 : spriteset_PW4ue_purchase_KPEV;
							1920 .. 1945 : spriteset_PW4ue_purchase_DRG;
							1946 .. 2050 : spriteset_PW4ue_purchase_DB;
								spriteset_PW4ue_purchase_DB;}
									
						switch (FEAT_TRAINS, SELF, random_choose_PW4ue_KPEV, switch_alternative_var_random_bits(1, 1)) {
							0: spritegroup_PW4ue_GW_KPEV;
							1: spritegroup_PW4ue_NGW_KPEV;}
							
						switch (FEAT_TRAINS, SELF, random_choose_PW4ue_DRG, switch_alternative_var_random_bits(1, 1)) {
							0: spritegroup_PW4ue_GW_DRG;
							1: spritegroup_PW4ue_NGW_DRG;}							
								
						switch (FEAT_TRAINS,SELF,switch_build_year_PW4ue,build_year) {
							   0 .. 1919 : random_choose_PW4ue_KPEV;
							1920 .. 1945 : random_choose_PW4ue_DRG;
							1946 .. 2050 : spritegroup_PW4ue_GW_DB;
								spritegroup_PW4ue_GW_DB;}

						switch (FEAT_TRAINS,SELF,switch_current_year_PW4ue,current_year) {
							   0 .. 1919 : random_choose_PW4ue_KPEV;
							1920 .. 1945 : random_choose_PW4ue_DRG;
							1946 .. 2050 : spritegroup_PW4ue_GW_DB;
								spritegroup_PW4ue_GW_DB;}								
					
						switch(FEAT_TRAINS, SELF, switch_length_PW4ue, position_in_vehid_chain % 3) {
							0: 4;
							1: 4;
							2: 4;}
						
						switch(FEAT_TRAINS,PARENT,switch_running_cost_PW4ue, 1){
							return 1200;}
							
						switch(FEAT_TRAINS, SELF, switch_PW4ue_graphics_build, position_in_vehid_chain  % 3) {
							0:     spriteset_empty;
							1:     return switch_build_year_PW4ue;
							2:     spriteset_empty;}	

						switch (FEAT_TRAINS, SELF, switch_articulated_PW4ue_build, extra_callback_info1) {
							1 .. 2 : return ITEM_PW4ue_BUILD;				
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}
																					
item (FEAT_TRAINS , ITEM_PW4ue_BUILD, 5825)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_PW4UE_BUILD);
