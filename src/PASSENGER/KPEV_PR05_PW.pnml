						reliability_decay: 				20;
						refittable_cargo_classes:		bitmask();
						cargo_allow_refit:				[MAIL, GOOD, VALU];
						loading_speed:					8;
						cost_factor:                    108;
						running_cost_base:              RUNNING_COST_DIESEL;
						running_cost_factor: 			90;
						
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							120 km/h;
						refit_cost: 					0;
						track_type: 					RAIL;						
						weight: 						41 ton;
						dual_headed: 					0;
						default_cargo_type:				MAIL;
						cargo_capacity:					32;
						ai_engine_rank: 				0;
						length: 						8;
												
			}
			
	//Vehicels Graphics
			graphics
			{
			default							: switch_PW4ue_graphics_build;
			additional_text					: string(STR_DESC_BAGG_EXP);
			length							: switch_length_PW4ue;
			articulated_part				: switch_articulated_PW4ue_build;
