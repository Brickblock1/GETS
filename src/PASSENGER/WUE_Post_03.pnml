							0: 4;
							1: 3;
							2: 4;}
						
						switch(FEAT_TRAINS,PARENT,switch_running_cost_Wue_POSTCI, 1){
							return 1200;}																					
							
						switch(FEAT_TRAINS, SELF, switch_Wue_POSTCI_graphics_build, position_in_vehid_chain  % 3) {
							0:     spriteset_empty;
							1:     return switch_build_year_Wue_POSTCI;
							2:     spriteset_empty;}

						switch (FEAT_TRAINS, SELF, switch_articulated_Wue_POSTCI_build, extra_callback_info1) {
							1 .. 2: return ITEM_WUE_POSTCI_BUILD;					
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}

item (FEAT_TRAINS , ITEM_WUE_POSTCI_BUILD, 3580)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_WUE_POSTCI_BUILD);
						extra_flags:					bitmask(VEHICLE_FLAG_SYNC_VARIANT_EXCLUSIVE_PREVIEW);
						climates_available: 			ALL_CLIMATES;
						introduction_date: 				date(1903,01,01);
						model_life: 					55;
						vehicle_life: 					40;
