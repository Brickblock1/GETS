						reliability_decay: 				20;
						misc_flags: 					bitmask(TRAIN_FLAG_SPRITE_STACK);
						refittable_cargo_classes:		bitmask();
						cargo_allow_refit:				[MAIL, GOOD, VALU];
						loading_speed:					8;
						cost_factor:                    108;
						running_cost_base:              RUNNING_COST_DIESEL;
						running_cost_factor: 			90;
												
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							80 km/h;
						refit_cost: 					0;
						track_type: 					RAIL;						
						weight: 						28 ton;
						dual_headed: 					0;
						default_cargo_type:				MAIL;
						cargo_capacity:					36;
						ai_engine_rank: 				0;
						length: 						8;
												
			}
			
	//Vehicels Graphics
			graphics
			{
			default							: switch_Wue_POSTCI_graphics_build;
			additional_text					: string(STR_DESC_POST);
			length							: switch_length_Wue_POSTCI;
			articulated_part				: switch_articulated_Wue_POSTCI_build;
			purchase						: switch_purchase_Wue_POSTCI;
			cost_factor						: 230;
			running_cost_factor				: switch_running_cost_Wue_POSTCI;			

			 }
		}

							
						switch(FEAT_TRAINS, SELF, switch_Wue_POSTCI_graphics_current, position_in_vehid_chain  % 3) {
