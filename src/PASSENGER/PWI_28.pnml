							   0 .. 1945 : spritegroup_Pwi28_DRG;
							1946 .. 2050 : spritegroup_Pwi28_DB;
							spritegroup_Pwi28_DB;}							
							
						switch(FEAT_TRAINS,PARENT,switch_running_cost_Pwi28, 1){
							return 1200;}
						
						
item (FEAT_TRAINS , ITEM_PWI28_BUILD, 5750)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_PWI28_BUILD);
						extra_flags:					bitmask(VEHICLE_FLAG_SYNC_VARIANT_EXCLUSIVE_PREVIEW);
						climates_available: 			ALL_CLIMATES;
						introduction_date: 				date(1923,01,01);
						model_life: 					53;
						vehicle_life: 					40;
						retire_early:					-10;						
						reliability_decay: 				20;
						refittable_cargo_classes:		bitmask();
						cargo_allow_refit:				[MAIL, GOOD, VALU];
						loading_speed:					8;
						cost_factor:                    108;
						running_cost_base:              RUNNING_COST_DIESEL;
						running_cost_factor: 			90;
						
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							90 km/h;
						refit_cost: 					0;
						track_type: 					RAIL;						
						weight: 						20 ton;
						dual_headed: 					0;
						default_cargo_type:				MAIL;
						cargo_capacity:					116;
						ai_engine_rank: 				0;
						length: 						8;
												
			}
			
	//Vehicels Graphics
			graphics{
			default							: switch_build_year_Pwi28;
			additional_text					: string(STR_DESC_BAGG);
			purchase						: switch_purchase_Pwi28;
			cost_factor						: 250;
			running_cost_factor				: switch_running_cost_Pwi28;
			}
		}

