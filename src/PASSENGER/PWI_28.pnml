//Switches

						switch (FEAT_TRAINS, SELF, switch_purchase_Pwi28, current_year){
							   0 .. 1945 : spriteset_Pwi28_purchase1;
							1946 .. 2050 : spriteset_Pwi28_purchase2;
							spriteset_Pwi28_purchase2;}	

						switch (FEAT_TRAINS, SELF, switch_build_year_Pwi28, build_year){
							   0 .. 1945 : spritegroup_Pwi28_DRG;
							1946 .. 2050 : spritegroup_Pwi28_DB;
							spritegroup_Pwi28_DB;}
							
