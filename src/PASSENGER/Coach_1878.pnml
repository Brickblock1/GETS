//Switches

					switch (FEAT_TRAINS,SELF, switch_Coach_1878_purchase,current_year){
						    0 .. 1919: spriteset_Coach_1878_KPEV_purchase;
						 1920 .. 2050: spriteset_Coach_1878_DRG_purchase;						 
							spriteset_Coach_1878_DRG_purchase;}
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1878_KPEV_AB, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1878_KPEV_AB;
							1: spritegroup_Coach_1878_loading;}																									

						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1878_KPEV_C, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1878_KPEV_C;
							1: spritegroup_Coach_1878_loading;}
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1878_KPEV_D, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
