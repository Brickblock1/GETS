						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1878_DRG_AB, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1878_DRG_AB;
							1: spritegroup_Coach_1878_loading;}																									

						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1878_DRG_C, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1878_DRG_C;
							1: spritegroup_Coach_1878_loading;}
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_Coach_1878_DRG_D, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_Coach_1878_DRG_D;
							1: spritegroup_Coach_1878_loading;}																		

					switch (FEAT_TRAINS, SELF, random_choose_Coach_1878_KPEV, switch_alternative_var_random_bits(2, 2)) {
							1: switch_spritestack_Coach_1878_KPEV_AB;
							2: switch_spritestack_Coach_1878_KPEV_C;
							3:switch_spritestack_Coach_1878_KPEV_D;											
								switch_spritestack_Coach_1878_KPEV_D;}

					switch (FEAT_TRAINS, SELF, random_choose_Coach_1878_DRG, switch_alternative_var_random_bits(2, 2)) {
							1: switch_spritestack_Coach_1878_DRG_AB;
							2: switch_spritestack_Coach_1878_DRG_C;
							3:switch_spritestack_Coach_1878_DRG_D;											
								switch_spritestack_Coach_1878_DRG_D;}
								
					switch(FEAT_TRAINS,PARENT,switch_running_cost_Coach_1878, 1){
						return 675;}								
								
					switch (FEAT_TRAINS,SELF,switch_current_year_Coach_1878,current_year) {
							   0 .. 1919 : random_choose_Coach_1878_KPEV;
							1920 .. 2050 : random_choose_Coach_1878_DRG;
								random_choose_Coach_1878_DRG;}
						
					switch (FEAT_TRAINS,SELF,switch_build_year_Coach_1878,build_year) {
							   0 .. 1919 : random_choose_Coach_1878_KPEV;
							1920 .. 2050 : random_choose_Coach_1878_DRG;
								random_choose_Coach_1878_DRG;}			
								
