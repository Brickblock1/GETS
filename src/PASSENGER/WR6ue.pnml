spriteset (spriteset_WR6ue_purchase_Teak, "gfx/WR6ue/WR6ue_Teak_1x_8bpp.png")
					{ template_purchase(0, 0)}	
				
		alternative_sprites (spriteset_WR6ue_purchase_Teak, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx/WR6ue/WR6ue_Teak_1x_32bpp.png", "gfx/WR6ue/WR6ue_Teak_1x_mask.png")
