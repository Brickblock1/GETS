spriteset (spriteset_WR6ue_purchase_DRG, "gfx/WR6ue/WR6ue_DRG_1x_8bpp.png")
					{ template_purchase(0, 0)}	
				
		alternative_sprites (spriteset_WR6ue_purchase_DRG, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx/WR6ue/WR6ue_DRG_1x_32bpp.png", "gfx/WR6ue/WR6ue_DRG_1x_mask.png")
					{template_purchase(0,0)}
					
//Switches


						switch (FEAT_TRAINS,SELF,switch_purchase_WR6ue,current_year) {
							0 .. 1919 : spriteset_WR6ue_purchase_Teak;
							1920 .. 2050 : spriteset_WR6ue_purchase_DRG;
								spriteset_WR6ue_purchase_DRG;}	
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_WR6ue_KPEV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_WR6ue_Teak;
							1: spritegroup_WR6ue_Teak_loading;}											

						switch(FEAT_TRAINS, SELF, switch_spritestack_WR6ue_DRG, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_WR6ue_DRG;
							1: spritegroup_WR6ue_DRG_loading;}
																			
						switch(FEAT_TRAINS, SELF, switch_length_WR6ue, position_in_vehid_chain % 3) {
							0: 4;
							1: 5;
							2: 4;}
						
						switch(FEAT_TRAINS,PARENT,switch_running_cost_WR6ue, 1){
							return 7500;}	
							
						switch (FEAT_TRAINS,SELF,switch_build_year_WR6ue,build_year) {
							   0 .. 1919 : switch_spritestack_WR6ue_KPEV;
							1920 .. 2050 : switch_spritestack_WR6ue_DRG;
								switch_spritestack_WR6ue_DRG;}		

						switch(FEAT_TRAINS, SELF, switch_WR6ue_graphics_build, position_in_vehid_chain  % 3) {
							0:     spriteset_empty;
							1:     return switch_build_year_WR6ue;
							2:     spriteset_empty;}	

						switch (FEAT_TRAINS, SELF, switch_articulated_WR6ue_build, extra_callback_info1) {
							1 ..2: return ITEM_WR6UE_BUILD;
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}														

item (FEAT_TRAINS , ITEM_WR6UE_BUILD, 5511)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_WR6UE_BUILD);
						extra_flags:					bitmask(VEHICLE_FLAG_SYNC_VARIANT_EXCLUSIVE_PREVIEW);
						climates_available: 			ALL_CLIMATES;
						introduction_date: 				date(1907,01,01);
						model_life: 					30;
						vehicle_life: 					30;
						retire_early:					-10;						
						reliability_decay: 				20;
						misc_flags: 					bitmask(TRAIN_FLAG_SPRITE_STACK);
						refittable_cargo_classes:		bitmask(CC_PASSENGERS);
						cargo_allow_refit:				[PASS, TOUR];
						loading_speed:					9;
						cost_factor:                    108;
						running_cost_base:              RUNNING_COST_DIESEL;
						running_cost_factor: 			90;
						cargo_age_period:				232;
						
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							120 km/h;
						refit_cost: 					0;
						track_type: 					RAIL;						
						weight: 						50 ton;
						dual_headed: 					0;
						default_cargo_type:				PASS;
						cargo_capacity:					13;
						ai_engine_rank: 				0;
						length: 						8;												
			}
			
	//Vehicels Graphics
			graphics
			{
			default							: switch_WR6ue_graphics_build;
			additional_text					: string(STR_DESC_RESTAURANT);
			length							: switch_length_WR6ue;
			articulated_part				: switch_articulated_WR6ue_build;
			purchase						: switch_purchase_WR6ue;
			cost_factor						: 750;
			running_cost_factor				: switch_running_cost_WR6ue;				
			 }
		}

						switch (FEAT_TRAINS,SELF,switch_current_year_WR6ue,current_year) {
							   0 .. 1919 : switch_spritestack_WR6ue_KPEV;
							1920 .. 2050 : switch_spritestack_WR6ue_DRG;
								switch_spritestack_WR6ue_DRG;}		

						switch(FEAT_TRAINS, SELF, switch_WR6ue_graphics_current, position_in_vehid_chain  % 3) {
							0:     spriteset_empty;
							1:     return switch_current_year_WR6ue;
							2:     spriteset_empty;}	

						switch (FEAT_TRAINS, SELF, switch_articulated_WR6ue_current, extra_callback_info1) {
							1 ..2: return ITEM_WR6UE_CURRENT;
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}														

