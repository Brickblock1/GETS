						switch (FEAT_TRAINS, SELF, switch_purchase_DOSTO_1GEN_DVT, current_year){
							   0 .. 1995 : spriteset_DOSTO_1GEN_DVT_purchase1;
							1996 .. 2050 : spriteset_DOSTO_1GEN_DVT_purchase2;
								spriteset_DOSTO_1GEN_DVT_purchase2;}

