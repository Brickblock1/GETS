							1: spritegroup_DOSTO_1GEN_DVT_mint_loading;}

						switch(FEAT_TRAINS, SELF, switch_loadstack_DOSTO_1GEN_DVT_verkehrsrot, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
