							1: spritegroup_DOSTO_1GEN_DVT_verkehrsrot_loading;}							
																							
						switch(FEAT_TRAINS, SELF, switch_length_DOSTO_1GEN_DVT, position_in_vehid_chain % 3) {
							0: 4;
							1: 7;
							2: 4;}					
															
						switch (FEAT_TRAINS, SELF, switch_build_year_DOSTO_1GEN_DVT, build_year){
							   0 .. 1995 : switch_loadstack_DOSTO_1GEN_DVT_mint;
							1996 .. 2050 : switch_loadstack_DOSTO_1GEN_DVT_verkehrsrot;
								switch_loadstack_DOSTO_1GEN_DVT_verkehrsrot;}	
														
						switch(FEAT_TRAINS, SELF, switch_DOSTO_1GEN_DVT_graphics_build, position_in_vehid_chain  % 3) {
							0:     spriteset_empty;
							1:     return switch_build_year_DOSTO_1GEN_DVT;
							2:     spriteset_empty;}	

						switch (FEAT_TRAINS, SELF, switch_articulated_DOSTO_1GEN_DVT_build, extra_callback_info1) {
							1 .. 2: return ITEM_DOSTO_1GEN_DVT_BUILD;	
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}

