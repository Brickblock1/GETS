							1: spritegroup_VT_125_middle1_loading;}
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_VT_125_middle2, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_VT_125_middle2;
							1: spritegroup_VT_125_middle2_loading;}
							
