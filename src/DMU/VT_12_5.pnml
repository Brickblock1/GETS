							0: spriteset_VT_125_back;
							1: spritegroup_VT_125_back_loading;}																					

						switch(FEAT_TRAINS, SELF, switch_VT_125_3_graphics, position_in_vehid_chain % 9) {
							0:	   spriteset_empty;
							1:     switch_spritestack_VT_125_front;
							2:     spriteset_empty;
							3:	   spriteset_empty;
							4:	   switch_spritestack_VT_125_middle1;
							5:	   spriteset_empty;
							6:	   spriteset_empty;
							7:	   switch_spritestack_VT_125_back;
							8:	   spriteset_empty;}													

						switch(FEAT_TRAINS, SELF, switch_length_VT_125_3, position_in_vehid_chain % 9) {
							0: 4;
							1: 7;
							2: 4;
							3: 4;
							4: 7;
							5: 4;
							6: 4;
							7: 7;
							8: 4;}
							
						switch (FEAT_TRAINS, SELF, switch_articulated_VT_125_3, extra_callback_info1) {
							1 .. 8: return ITEM_VT_125_3; 
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}

						switch(FEAT_TRAINS,PARENT,switch_running_cost_VT_125_3, 1){
							return 2375;}	

