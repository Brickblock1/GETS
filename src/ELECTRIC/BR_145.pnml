							PASS: spriteset_BR_145_verkehrsrot;
							MAIL: spriteset_BR_145_verkehrsrot;
								switch_BR_145_current_year;}
						
						switch(FEAT_TRAINS, SELF, switch_cargo_dependant_BR_145_build, most_common_cargo_type){
							PASS: spriteset_BR_145_verkehrsrot;
							MAIL: spriteset_BR_145_verkehrsrot;
								switch_BR_145_build_year;}																					
					
