							1: spriteset_E_32_purchase_anim;}

						switch(FEAT_TRAINS, SELF, switch_spritestack_E_32_purchase2, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
