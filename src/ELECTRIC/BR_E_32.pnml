							1: spriteset_E_32_purchase_anim;}														

						switch(FEAT_TRAINS,SELF, switch_purchase_E_32, current_year){
							1900 .. 1926: switch_spritestack_E_32_purchase1;
							1927 .. 1945: switch_spritestack_E_32_purchase2;
							1946 .. 1990: switch_spritestack_E_32_purchase3;
							switch_spritestack_E_32_purchase3;}

						switch (FEAT_TRAINS, SELF, switch_animation_E_32, motion_counter % 4) {
								0: spriteset_E_32_anim4;
								1: spriteset_E_32_anim3;
								2: spriteset_E_32_anim2;
								3: spriteset_E_32_anim1;}
								
						switch(FEAT_TRAINS, SELF, switch_spritestack_E_32_BAY, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_E_32_BAY;
							1: return switch_animation_E_32;}		
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_E_32_DRG, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_E_32_DRG;
							1: return switch_animation_E_32;}	
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_E_32_DB, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_E_32_DB;
							1: return switch_animation_E_32;}																																			
									
						switch (FEAT_TRAINS,SELF, switch_E_32_current_year,current_year){
							 1920 .. 1926: switch_spritestack_E_32_BAY;	
							 1927 .. 1945: switch_spritestack_E_32_DRG;
							 1946 .. 1990: switch_spritestack_E_32_DB;							 					 					 
								switch_spritestack_E_32_DB;}
		
						switch (FEAT_TRAINS,SELF, switch_E_32_build_year,build_year){
							 1920 .. 1926: switch_spritestack_E_32_BAY;	
							 1927 .. 1945: switch_spritestack_E_32_DRG;
							 1946 .. 1990: switch_spritestack_E_32_DB;							 					 					 
								switch_spritestack_E_32_DB;}
							
						switch(FEAT_TRAINS,PARENT,switch_running_cost_E_32, 1){
						return 1025;}
						
						switch(FEAT_TRAINS,PARENT,switch_speed_E_32,current_year){
						1920 .. 1935 : 75 ;
						1936 .. 1990 : 90 ;	}					
						
						
