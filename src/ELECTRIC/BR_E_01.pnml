							1: return switch_animation_BR_E_01;}									

						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_E_01_DRG_REV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_E_01_DRG_REV;
							1: return switch_animation_BR_E_01_REV;}

						switch (FEAT_TRAINS, PARENT, switch_reverse_BR_E_01_KPEV, vehicle_is_reversed) {
							0: switch_spritestack_BR_E_01_KPEV;
								switch_spritestack_BR_E_01_KPEV_REV;}

