							1: spriteset_BR_E_01_anim_purchase;}												
						
						switch (FEAT_TRAINS,SELF, switch_BR_E_01_purchase,current_year){
							1900 .. 1919: switch_spritestack_BR_E_01_purchase1;
							1920 .. 2050: switch_spritestack_BR_E_01_purchase2;														 					 
								switch_spritestack_BR_E_01_purchase2;}	

						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_E_01_KPEV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_E_01_KPEV;
							1: return switch_animation_BR_E_01;}									

						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_E_01_KPEV_REV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
