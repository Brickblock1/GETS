							0: switch_spritestack_BR_E_01_DRG;
								switch_spritestack_BR_E_01_DRG_REV;}
						

						switch(FEAT_TRAINS,PARENT,switch_running_cost_BR_E_01, 1){
							return 1375;}
							
						switch (FEAT_TRAINS,SELF, switch_BR_E_01_build_year,build_year){
							1900 .. 1919: switch_reverse_BR_E_01_KPEV;
							1920 .. 2050: switch_reverse_BR_E_01_DRG;						 					 
								switch_reverse_BR_E_01_DRG;}													
						
