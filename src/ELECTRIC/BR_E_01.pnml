							1: return switch_animation_BR_E_01_REV;}

						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_E_01_DRG, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
