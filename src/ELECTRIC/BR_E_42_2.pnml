spriteset(spriteset_E_42_2_purchase1, "gfx/E_42_2/E_42_2_KPEV_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_E_42_2_purchase1, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx/E_42_2/E_42_2_KPEV_1x_32bpp.png", "gfx/E_42_2/E_42_2_KPEV_1x_mask.png")
