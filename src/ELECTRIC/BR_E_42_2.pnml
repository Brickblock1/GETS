spriteset(spriteset_E_42_2_purchase2, "gfx/E_42_2/E_42_2_DRG_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_E_42_2_purchase2, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx/E_42_2/E_42_2_DRG_1x_32bpp.png", "gfx/E_42_2/E_42_2_DRG_1x_mask.png")
									{template_purchase(0, 0)}																		
									
//Switches
						switch(FEAT_TRAINS, SELF, switch_spritestack_E_42_2_purchase1, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_E_42_2_purchase1;
							1: spriteset_E_42_2_purchase_anim;}

						switch(FEAT_TRAINS, SELF, switch_spritestack_E_42_2_purchase2, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_E_42_2_purchase2;
							1: spriteset_E_42_2_purchase_anim;}													

						switch(FEAT_TRAINS,SELF, switch_purchase_E_42_2, current_year){
							1900 .. 1926: switch_spritestack_E_42_2_purchase1;
							1927 .. 1945: switch_spritestack_E_42_2_purchase2;
							switch_spritestack_E_42_2_purchase2;}

						switch (FEAT_TRAINS, SELF, switch_animation_E_42_2, motion_counter % 4) {
								0: spriteset_E_42_2_anim4;
								1: spriteset_E_42_2_anim3;
								2: spriteset_E_42_2_anim2;
								3: spriteset_E_42_2_anim1;}
								
						switch(FEAT_TRAINS, SELF, switch_spritestack_E_42_2_KPEV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_E_42_2_KPEV;
							1: return switch_animation_E_42_2;}		
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_E_42_2_DRG, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_E_42_2_DRG;
							1: return switch_animation_E_42_2;}																																			
									
						switch (FEAT_TRAINS,SELF, switch_E_42_2_current_year,current_year){
							 1920 .. 1926: switch_spritestack_E_42_2_KPEV;	
							 1927 .. 2045: switch_spritestack_E_42_2_DRG;						 					 					 
								switch_spritestack_E_42_2_DRG;}
		
						switch (FEAT_TRAINS,SELF, switch_E_42_2_build_year,build_year){
							 1920 .. 1926: switch_spritestack_E_42_2_KPEV;	
							 1927 .. 2045: switch_spritestack_E_42_2_DRG;						 					 					 
								switch_spritestack_E_42_2_DRG;}																							

						switch(FEAT_TRAINS,PARENT,switch_running_cost_E_42_2, 1){
							return 575;}						
						
