//Switches for Two-Part Consist																																

						switch(FEAT_TRAINS, SELF, switch_BR_465_2_length, position_in_vehid_chain % 6) {
							0: 4;
							1: 5;
							2: 4;
							3: 4;
							4: 5;
							5: 4;}	


						switch(FEAT_TRAINS, SELF, switch_BR_465_2_FX_A, position_in_vehid_chain % 6) {
							0: return visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC, -5, DISABLE_WAGON_POWER);
							1 : return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							2 : return visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC, -2, DISABLE_WAGON_POWER);
							3 .. 5 : return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);}

						switch(FEAT_TRAINS, SELF, switch_BR_465_2_FX_B, position_in_vehid_chain % 6) {
							0 .. 2: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							3 : return visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC, -2, DISABLE_WAGON_POWER);
							4 : return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							5 : return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, -5, DISABLE_WAGON_POWER);}

						switch (FEAT_TRAINS, PARENT, switch_BR_465_2_FX, vehicle_is_reversed) {
							0: switch_BR_465_2_FX_A;
								switch_BR_465_2_FX_B;}
								
								
						switch(FEAT_TRAINS,PARENT,switch_running_cost_BR_465_2, 1){
							return 900;}								

						switch(FEAT_TRAINS, SELF, switch_BR_465_2_graphics, position_in_vehid_chain % 6) {
