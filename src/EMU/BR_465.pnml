							2: return random_choose_BR_465_back_ADV_REV;}																						

						switch (FEAT_TRAINS, PARENT, switch_reverse_BR_465_front, vehicle_is_reversed) {
							0: switch_spritestack_BR_465_front;
								switch_spritestack_BR_465_back_REV;}
								
						switch (FEAT_TRAINS, PARENT, switch_reverse_BR_465_back, vehicle_is_reversed) {
