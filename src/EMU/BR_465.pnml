							1: spriteset_BR_465_back_Jaeger_REV;
								spriteset_empty;}																								

						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_465_front, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_465_front;
							1: spritegroup_ET_65_front_loading;
							2: return random_choose_BR_465_front_ADV;}	
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_465_front_REV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_465_front_REV;
							1: spritegroup_ET_65_front_loading_REV;
							2: return random_choose_BR_465_front_ADV_REV;}	
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_465_middle, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
