							1: spritegroup_BR_425_middle_load;}
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_425_middle2, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_425_middle2;
							1: spritegroup_BR_425_middle_load;}	
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_425_back, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_425_back;
							1: spritegroup_BR_425_back_load;}
							
					switch(FEAT_TRAINS, SELF, switch_BR_425_graphics, position_in_vehid_chain % 12) {
							0:	   spriteset_empty;
							1:     return switch_spritestack_BR_425_front;
							2:     spriteset_empty;
							3:	   spriteset_empty;
							4:	   return switch_spritestack_BR_425_middle1;
							5:	   spriteset_empty;							
							6:	   spriteset_empty;
							7:	   return switch_spritestack_BR_425_middle2;	
							8:	   spriteset_empty;
							9:	   spriteset_empty;
						   10:     return switch_spritestack_BR_425_back;
						   11:     spriteset_empty;}													 							 						 													

					switch(FEAT_TRAINS, SELF, switch_length_BR_425, position_in_vehid_chain % 12) {
						0: 4;
						1: 3;
						2: 4;
						3: 3;
						4: 3;
						5: 3;
						6: 3;						
						7: 3;
						8: 3;
						9: 4;
						10: 3;
						11: 4;}							

						   
					switch (FEAT_TRAINS, SELF, switch_articulated_BR_425, extra_callback_info1) {
						1 .. 11: return ITEM_BR_425; 
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}	

					switch(FEAT_TRAINS,PARENT,switch_running_cost_BR_425, 1){
							return 2050;}
							
					switch(FEAT_TRAINS, SELF, switch_FX_BR_425, position_in_vehid_chain % 12) {
							0: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							1: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							2: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							3: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							4: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							5: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);								
							6: return visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC,-2, DISABLE_WAGON_POWER);
							7: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							8: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
							9: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);}							

					switch(FEAT_TRAINS, SELF, switch_coupling_BR_425, vehicle_type_id) {
							4255: return CB_RESULT_ATTACH_ALLOW;
							4230: return CB_RESULT_ATTACH_ALLOW;
							4231: return CB_RESULT_ATTACH_ALLOW;
							4232: return CB_RESULT_ATTACH_ALLOW;
							4233: return CB_RESULT_ATTACH_ALLOW;
							4234: return CB_RESULT_ATTACH_ALLOW;
							4240: return CB_RESULT_ATTACH_ALLOW;
							4260: return CB_RESULT_ATTACH_ALLOW;
							return string (STR_COUPLING_SAME);}							

