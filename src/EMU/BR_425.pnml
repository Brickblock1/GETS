									{ template_purchase(0, 0)}			
			
			
			//Switches

						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_425_front, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_425_front;
							1: spritegroup_BR_425_front_load;}									

						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_425_middle1, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
