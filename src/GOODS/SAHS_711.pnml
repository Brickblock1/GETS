							spriteset_Sahs_711_purchase_braun;}							

					switch(FEAT_TRAINS, SELF, switch_loadstack_Sahs_711_VAR1_schwarz, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
						0: spriteset_Sahs_711_VAR1_schwarz;
						1: spritegroup_Sahs_711_VAR1_loading;}	

					switch(FEAT_TRAINS, SELF, switch_loadstack_Sahs_711_VAR2_schwarz, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
						0: spriteset_Sahs_711_VAR2_schwarz;
						1: spritegroup_Sahs_711_VAR2_loading;}	

					switch(FEAT_TRAINS, SELF, switch_loadstack_Sahs_711_VAR1_braun, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
