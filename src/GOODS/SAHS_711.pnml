						1: spritegroup_Sahs_711_VAR1_loading;}	

					switch(FEAT_TRAINS, SELF, switch_loadstack_Sahs_711_VAR2_braun, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
						0: spriteset_Sahs_711_VAR2_braun;
						1: spritegroup_Sahs_711_VAR2_loading;}	

					switch (FEAT_TRAINS, SELF, random_choose_Sahs_711_schwarz, switch_alternative_var_random_bits(1, 1)) {
						1: switch_loadstack_Sahs_711_VAR1_schwarz;
							switch_loadstack_Sahs_711_VAR2_schwarz;}	

					switch (FEAT_TRAINS, SELF, random_choose_Sahs_711_braun, switch_alternative_var_random_bits(1, 1)) {
						1: switch_loadstack_Sahs_711_VAR1_braun;
							switch_loadstack_Sahs_711_VAR2_braun;}

					switch(FEAT_TRAINS, SELF, switch_length_Sahs_711, position_in_vehid_chain % 3) {
						0: 3;
						1: 4;
						2: 3;}							
																				
					switch(FEAT_TRAINS,PARENT,switch_running_cost_Sahs_711, 1){
						return 600;}

					switch (FEAT_TRAINS,SELF, switch_Sahs_711_current_year,current_year){
						00 .. 1986: random_choose_Sahs_711_schwarz;
					  1987 .. 2050: random_choose_Sahs_711_braun;					 					 
							random_choose_Sahs_711_braun;}
								
					switch(FEAT_TRAINS, SELF, switch_Sahs_711_current_graphics, position_in_articulated_veh % 3) {
						0:	   spriteset_empty;
						1:     return switch_Sahs_711_current_year;
						2:	   spriteset_empty;}

					switch (FEAT_TRAINS, SELF, switch_articulated_Sahs_711_current, extra_callback_info1) {
						1 .. 2: return ITEM_SAHS_711_CURRENT;
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}	
																			
