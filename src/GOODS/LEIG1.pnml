//Switches

					switch (FEAT_TRAINS,SELF, switch_LEIG1_purchase,current_year){
						   0 .. 1945: spriteset_LEIG1_purchase1;
						1946 .. 2055: spriteset_LEIG1_purchase2;				 					 
