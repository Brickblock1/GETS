						   0 .. 1945 : spritegroup_LEIG1_front_DRG;
						1946 .. 2000 : spritegroup_LEIG1_front_DB;
						spritegroup_LEIG1_front_DB;}		
						
					switch (FEAT_TRAINS,SELF,switch_graphics_LEIG1_back,current_year) {
