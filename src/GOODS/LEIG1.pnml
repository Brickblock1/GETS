						1946 .. 2000 : spritegroup_LEIG1_back_DB;
						spritegroup_LEIG1_back_DB;}
						
					switch(FEAT_TRAINS, SELF, switch_LEIG1_graphics, position_in_articulated_veh % 2) {
						0:	   switch_graphics_LEIG1_front;
						1:     switch_graphics_LEIG1_back;}

					switch(FEAT_TRAINS, SELF, switch_length_LEIG1, position_in_articulated_veh % 2) {
						0: 8;
						1: 8;}

					switch (FEAT_TRAINS, SELF, switch_articulated_LEIG1, extra_callback_info1) {
						1: return ITEM_LEIG1;
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}

					switch(FEAT_TRAINS,PARENT,switch_running_cost_LEIG1, 1){
						return 550;}								

