			}
			
	//Vehicels Graphics
			graphics
			{
			default							: switch_Lgjs_571_graphics;
			additional_text					: string(STR_DESC_GOODS);
			articulated_part			  	: switch_articulated_Lgjs_571;
			length							: switch_length_Lgjs_571;
			purchase						: spriteset_Lgjs_571_purchase;
			cost_factor						: 102;
			running_cost_factor				: switch_running_cost_Lgjs_571;
			colour_mapping					: switch_generic_recolour;
			 }
