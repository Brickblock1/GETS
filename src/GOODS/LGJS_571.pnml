spriteset (spriteset_Lgjs_571, "gfx\Lgjs_571/Lgjs_571_1x_8bpp.png")
					{template_98trains_1x(0, 0)}	
				
		alternative_sprites (spriteset_Lgjs_571, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx\Lgjs_571/Lgjs_571_1x_32bpp.png", "gfx\Lgjs_571/Lgjs_571_1x_mask.png")
					{template_98trains_1x(0, 0)}	
				
		alternative_sprites (spriteset_Lgjs_571, ZOOM_LEVEL_IN_2X, BIT_DEPTH_32BPP, "gfx\Lgjs_571/Lgjs_571_2x_32bpp.png", "gfx\Lgjs_571/Lgjs_571_2x_mask.png")
					{template_98trains_2x(0, 0)}					
			
		alternative_sprites (spriteset_Lgjs_571, ZOOM_LEVEL_IN_4X, BIT_DEPTH_32BPP, "gfx\Lgjs_571/Lgjs_571_4x_32bpp.png", "gfx\Lgjs_571/Lgjs_571_4x_mask.png")
						{template_98trains_4x(0, 0)}
						
//Spriteset purchase
															
spriteset (spriteset_Lgjs_571_purchase, "gfx\Lgjs_571/Lgjs_571_1x_8bpp.png")
					{template_purchase(0, 0)}	
				
		alternative_sprites (spriteset_Lgjs_571_purchase, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx\Lgjs_571/Lgjs_571_1x_32bpp.png", "gfx\Lgjs_571/Lgjs_571_1x_mask.png")
					{template_purchase(0, 0)}	
					
//Switches	

					switch(FEAT_TRAINS, SELF, switch_loadstack_Lgjs_571_Cont40_Dry, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
						0: spriteset_Lgjs_571;
						1: switch_Cont40_center_Dry_loadstage_current_year;}
						
					switch(FEAT_TRAINS, SELF, switch_loadstack_Lgjs_571_Cont20_Dry_direction1, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
						0: spriteset_Lgjs_571;
						1: switch_Cont20_center1_Dry_loadstage_current_year;
						2: switch_Cont20_center2_Dry_loadstage_current_year;}
						
					switch(FEAT_TRAINS, SELF, switch_loadstack_Lgjs_571_Cont20_Dry_direction2, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
						0: spriteset_Lgjs_571;
						1: switch_Cont20_center2_Dry_loadstage_current_year;
						2: switch_Cont20_center1_Dry_loadstage_current_year;}
						
					switch(FEAT_TRAINS, SELF, switch_loadstack_Lgjs_571_Cont20_Dry_direction, direction) {
							DIRECTION_SOUTHEAST: switch_loadstack_Lgjs_571_Cont20_Dry_direction1;
							DIRECTION_SOUTH: switch_loadstack_Lgjs_571_Cont20_Dry_direction1;
							DIRECTION_SOUTHWEST: switch_loadstack_Lgjs_571_Cont20_Dry_direction1;
								switch_loadstack_Lgjs_571_Cont20_Dry_direction2;}							
						
					random_switch(FEAT_TRAINS, SELF, switch_random_Cont_decider_Lgjs_571_Dry, bitmask(TRIGGER_VEHICLE_UNLOAD_ALL, TRIGGER_VEHICLE_NEW_LOAD,)) {
							independent: random_Cont40_center_Era1_Dry_loadstage;
							independent: random_Cont40_center_Era2_Dry_loadstage;
							independent: random_Cont20_center1_Era1_Dry_loadstage;
							independent: random_Cont20_center1_Era2_Dry_loadstage;					
							independent: random_Cont20_center2_Era1_Dry_loadstage;				
							independent: random_Cont20_center2_Era2_Dry_loadstage;
							1: switch_loadstack_Lgjs_571_Cont40_Dry;
							1: switch_loadstack_Lgjs_571_Cont20_Dry_direction;}

					switch(FEAT_TRAINS, SELF, switch_loadstack_Lgjs_571_Cold, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
						0: spriteset_Lgjs_571;
						1: random_Cont40_center_Era1_cold_loadstage;}
						
					switch(FEAT_TRAINS, SELF, switch_loadstack_Lgjs_571_Mail_direction1, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
						0: spriteset_Lgjs_571;
						1: switch_Cont20_center1_Mail_current_year;
						2: switch_Cont20_center2_Mail_current_year;}
						
					switch(FEAT_TRAINS, SELF, switch_loadstack_Lgjs_571_Mail_direction2, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
						0: spriteset_Lgjs_571;
						1: switch_Cont20_center2_Mail_current_year;
						2: switch_Cont20_center1_Mail_current_year;}
						
					switch(FEAT_TRAINS, SELF, switch_loadstack_Lgjs_571_Mail_direction, direction) {
							DIRECTION_SOUTHEAST: switch_loadstack_Lgjs_571_Mail_direction1;
							DIRECTION_SOUTH: switch_loadstack_Lgjs_571_Mail_direction1;
							DIRECTION_SOUTHWEST: switch_loadstack_Lgjs_571_Mail_direction1;
								switch_loadstack_Lgjs_571_Mail_direction2;}											

					switch (FEAT_TRAINS, SELF, switch_Lgjs_571_cargo_type, cargo_type_in_veh) {				
							FISH: return switch_loadstack_Lgjs_571_Cold;
							FOOD: return switch_loadstack_Lgjs_571_Cold;
							FRUT: return switch_loadstack_Lgjs_571_Cold;
							FRVG: return switch_loadstack_Lgjs_571_Cold;
							MILK: return switch_loadstack_Lgjs_571_Cold;
							MAIL: return switch_loadstack_Lgjs_571_Mail_direction;													
								switch_random_Cont_decider_Lgjs_571_Dry;}
								
						switch(FEAT_TRAINS, SELF, switch_Lgjs_571_graphics, position_in_articulated_veh % 3) {
							0:	   spriteset_empty;
							1:     return switch_Lgjs_571_cargo_type;
							2:	   spriteset_empty;}

						switch(FEAT_TRAINS, SELF, switch_length_Lgjs_571, position_in_vehid_chain % 3) {
							0: 3;
							1: 3;
							2: 3;}								
						 		 
					switch (FEAT_TRAINS, SELF, switch_articulated_Lgjs_571, extra_callback_info1) {
						1: return ITEM_LGJS_571;
						2: return ITEM_LGJS_571;
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}																																																																												

					switch(FEAT_TRAINS,PARENT,switch_running_cost_Lgjs_571, 1){
						return 205;}				

item (FEAT_TRAINS , ITEM_LGJS_571, 9851)
