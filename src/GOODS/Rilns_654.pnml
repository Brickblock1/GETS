						switch(FEAT_TRAINS, SELF, switch_length_Rilns_654, position_in_vehid_chain % 3) {
							0: 4;
							1: 4;
							2: 4;}																						

						switch(FEAT_TRAINS,PARENT,switch_running_cost_Rilns_654, 1){
							return 500;}	
							
						switch(FEAT_TRAINS, SELF, switch_Rilns_654_graphics_current, position_in_articulated_veh % 3) {
							0:	   spriteset_empty;
							1:     return switch_Rilns_654_current_year;
							2:	   spriteset_empty;}

						switch (FEAT_TRAINS, SELF, switch_articulated_Rilns_654_current, extra_callback_info1) {
							1 .. 2: return ITEM_RILNS_654_CURRENT;
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}	

