item (FEAT_TRAINS , ITEM_SAHIMMS_900_CURRENT, 9038)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_SAHIMMS_900_CURRENT);
						extra_flags:					bitmask(VEHICLE_FLAG_SYNC_VARIANT_EXCLUSIVE_PREVIEW);
						climates_available: 			ALL_CLIMATES;
						introduction_date: 				date(1981,01,01);
						model_life: 					VEHICLE_NEVER_EXPIRES;
						vehicle_life: 					40;
						retire_early:					-10;						
						reliability_decay: 				20;
						refittable_cargo_classes: 		bitmask(CC_COVERED, CC_PIECE_GOODS);
						non_refittable_cargo_classes: 	bitmask(CC_LIQUID, CC_BULK);
						cargo_allow_refit:              [STAL, ALUM, STCB, STST, STSH, STWR, COPR,];
						cargo_disallow_refit: 			[LVST, FOOD, WATR, BEER, BOOM, FISH, IRON, ENSP, FMSP, FRUT, PAPR, MPTS, POWR, VPTS, WOOD, WOOL, PIPE, VEHI, VBOD, VENG, MNSP, ZINC, CSTI, GLAS, STSE, TYRE];
						misc_flags: 					bitmask(TRAIN_FLAG_AUTOREFIT, TRAIN_FLAG_SPRITE_STACK);
						loading_speed:					6;
						cost_factor:                    108;
						running_cost_base:              RUNNING_COST_DIESEL;
						running_cost_factor: 			90;
						
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							120 km/h;
						refit_cost: 					0;
						track_type: 					RAIL;						
						weight: 						34 ton;
						dual_headed: 					0;
						default_cargo_type:				STAL;
						cargo_capacity:					28;
						ai_engine_rank: 				0;
						length: 						8;
												
			}
			
	//Vehicels Graphics
			graphics
			{
			default							: switch_Sahimms_900_current_graphics;
			additional_text					: string(STR_DESC_GOODS);
			articulated_part			  	: switch_articulated_Sahimms_900_current;
			length							: switch_length_Sahimms_900;
			purchase						: switch_Sahimms_900_purchase;
			cost_factor						: 260;
			running_cost_factor				: switch_running_cost_Sahimms_900;	
			colour_mapping					: switch_generic_recolour;
			 }
		}
								
					switch(FEAT_TRAINS, SELF, switch_Sahimms_900_braun_graphics, position_in_articulated_veh % 3) {
						0:	   spriteset_empty;
						1:     spritegroup_Sahimms_900_braun;
						2:	   spriteset_empty;}

