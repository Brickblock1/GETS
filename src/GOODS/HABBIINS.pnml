//Switches

					switch (FEAT_TRAINS, SELF, random_choose_Habbiins, switch_alternative_var_random_bits(1, 2)) {
							1: spritegroup_Habbiins_Railion;
							2: spritegroup_Habbiins_Transwaggon;
							3: spritegroup_Habbiins_DB;
								spritegroup_Habbiins_DB;}	

					switch (FEAT_TRAINS, SELF, random_choose_Habbiins_DB, switch_alternative_var_random_bits(1, 1)) {
							1: spritegroup_Habbiins_Railion;
								spritegroup_Habbiins_DB;}
													
					switch(FEAT_TRAINS, SELF, switch_length_Habbiins, position_in_vehid_chain % 3) {
							0: 4;
							1: 6;
							2: 4;}
						 		 
					switch(FEAT_TRAINS,PARENT,switch_running_cost_Habbiins, 1){
						return 450;}								

					switch(FEAT_TRAINS, SELF, switch_Habbiins_graphics, position_in_vehid_chain  % 3) {
							0:     spriteset_empty;
							1:     return random_choose_Habbiins;
							2:     spriteset_empty;}

					switch (FEAT_TRAINS, SELF, switch_articulated_Habbiins, extra_callback_info1) {
						1 .. 2: return ITEM_HABBIINS;
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}
													
item (FEAT_TRAINS , ITEM_HABBIINS, 8365)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_HABBIINS);
						extra_flags:					bitmask(VEHICLE_FLAG_SYNC_VARIANT_EXCLUSIVE_PREVIEW);
						climates_available: 			ALL_CLIMATES;
						introduction_date: 				date(2004,01,01);
						model_life: 					VEHICLE_NEVER_EXPIRES;
						vehicle_life: 					40;
						retire_early:					-10;						
						reliability_decay: 				20;
						refittable_cargo_classes:		bitmask(CC_PIECE_GOODS);
						cargo_allow_refit:              [MAIL,GOOD,VALU,PAPR,DIAM,BDMT,BEER,BOOM,BRCK,CERA,CERE,DYES,ENSP,FERT,FICR,FMSP,GLAS,JAVA,PLAS,TYRE,WDPR,WOOL,ZINC,ALUM,BAKE,CIGR,COAT,COBL,FURN,METL,MNSP,MPTS,NUTS,PCL_,PIPE,SEED,SESP,SOAP,STAL,STCB,STSE,STSH,STST,STWR,TATO,VENG,VPTS,BATT,BUBL,FZDR,TOYS,];
						loading_speed:					7;
						cost_factor:                    108;
						running_cost_base:              RUNNING_COST_DIESEL;
						running_cost_factor: 			90;
						
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							120 km/h;
						refit_cost: 					0;
						track_type: 					RAIL;						
						weight: 						27 ton;
						dual_headed: 					0;
						default_cargo_type:				GOOD;
						cargo_capacity:					42;
						ai_engine_rank: 				0;
						length: 						8;
												
			}
			
	//Vehicels Graphics
			graphics
			{
			default							: switch_Habbiins_graphics;
			additional_text					: string(STR_DESC_GOODS);
			articulated_part			  	: switch_articulated_Habbiins;
			length							: switch_length_Habbiins;			
			purchase						: spriteset_Habbiins_purchase;
			cost_factor						: 123;
			running_cost_factor				: switch_running_cost_Habbiins;				
			
			 }
		}

					switch(FEAT_TRAINS, SELF, switch_Habbiins_graphics_DB, position_in_vehid_chain  % 3) {
