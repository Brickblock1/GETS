//Spritesets purchase

spriteset(spriteset_BR_815_altrot_purchase, "gfx/BR_515/BR_815_altrot_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_BR_815_altrot_purchase, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx/BR_515/BR_815_altrot_1x_32bpp.png", "gfx/BR_515/BR_815_altrot_1x_mask.png")
