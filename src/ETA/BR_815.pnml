spriteset(spriteset_BR_815_ozeanblau_purchase, "gfx/BR_515/BR_815_ozeanblau_1x_8bpp.png")
									{ template_purchase(0, 0)}
				
		alternative_sprites (spriteset_BR_815_ozeanblau_purchase, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx/BR_515/BR_815_ozeanblau_1x_32bpp.png", "gfx/BR_515/BR_815_ozeanblau_1x_mask.png")
									{ template_purchase(0, 0)}

//Switches

						switch (FEAT_TRAINS, SELF, switch_purchase_BR_815, current_year){
							1900 .. 1974 : spriteset_BR_815_altrot_purchase;
							1975 .. 2100 : spriteset_BR_815_ozeanblau_purchase;										
							spriteset_BR_815_ozeanblau_purchase;}	
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_815_altrot, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_815_altrot;
							1: spritegroup_BR_815_loading;}	
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_815_ozeanblau, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_815_ozeanblau;
							1: spritegroup_BR_815_loading;}

						switch(FEAT_TRAINS, SELF, switch_length_BR_815, position_in_vehid_chain % 3) {
							0: 4;
							1: 7;
							2: 4;}

						switch(FEAT_TRAINS,PARENT,switch_running_cost_BR_815, 1){
							return 800;}							

						switch (FEAT_TRAINS, SELF, switch_build_year_BR_815, build_year){
							1900 .. 1974 : switch_spritestack_BR_815_altrot;
							1975 .. 2100 : switch_spritestack_BR_815_ozeanblau;											
								switch_spritestack_BR_815_ozeanblau;}
							
						switch(FEAT_TRAINS, SELF, switch_BR_815_graphics_build, position_in_vehid_chain % 3) {
							0:	   spriteset_empty;
							1:     return switch_build_year_BR_815;
							2:     spriteset_empty;}		

						switch (FEAT_TRAINS, SELF, switch_articulated_BR_815_build, extra_callback_info1) {
							1 .. 2 : return ITEM_BR_815_BUILD; 
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}

