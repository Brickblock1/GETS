							1:  return string(STR_LIVERY_current_year);
							2:	return string(STR_LIVERY_SILBERLING_BLK); 
							3:	return string(STR_LIVERY_SILBERLING_OB); 										
							return CB_RESULT_NO_TEXT;}
							
					switch(FEAT_TRAINS, SELF, switch_DVT_n_wagen_Hase_LOCO_graphics, position_in_vehid_chain  % 3) {
							0:     spriteset_empty;
							1:     return switch_sprite_choose_DVT_n_wagen_Hase_LOCO;
							2:     spriteset_empty;}																									
					
					switch(FEAT_TRAINS, SELF, switch_length_DVT_n_wagen_Hase_LOCO, position_in_vehid_chain % 3) {
							0: 4;
							1: 7;
							2: 4;}							

					switch(FEAT_TRAINS, SELF, switch_speed_DVT_n_wagen, current_year){
						1950 .. 1967: 120;
						1968 .. 2050: 140;
						140;}

					switch (FEAT_TRAINS, SELF, switch_articulated_DVT_n_wagen_Hase_LOCO_MAIL, extra_callback_info1) {
							1: return ITEM_DVT_n_wagen_Hase_LOCO_MAIL;
							2: return ITEM_DVT_n_wagen_Hase_MAILPART;
							return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}
							
					switch (FEAT_TRAINS, SELF, switch_articulated_DVT_n_wagen_Hase_LOCO, extra_callback_info1) {
							1: return ITEM_DVT_n_wagen_Hase_LOCO;
							2: return ITEM_DVT_n_wagen_Hase_LOCO;
							return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}							
						
					switch(FEAT_TRAINS,PARENT,switch_running_cost_DVT_n_wagen_Hase, 1){
							return 2350;}	


