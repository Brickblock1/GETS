						switch (FEAT_TRAINS,SELF,switch_current_year_uicx_d,current_year) {
						1950 .. 1973 : spritegroup_UICX_D_gruen;
						1974 .. 1987 : spritegroup_UICX_D_ozeanblau;
						1988 .. 1998 : spritegroup_UICX_D_grau;
						1999 .. 2050 : spritegroup_UICX_D_verkehrsrot;
						spritegroup_UICX_D_ozeanblau;
						}	
						
						
						//Manuelle Auswahl Spriteset
						switch (FEAT_TRAINS,SELF, switch_sprite_choose_uicx_d, cargo_subtype) {
							0:  switch_build_year_uicx_d;
							1:  switch_current_year_uicx_d;
							2:	spritegroup_UICX_D_gruen; 
							3:	spritegroup_UICX_D_ozeanblau;
							4:	spritegroup_UICX_D_grau;
							5:  spritegroup_UICX_D_mint;
							6:  spritegroup_UICX_D_verkehrsrot;
							switch_build_year_uicx_d;
							} 	
							
						//Auswahl Menu
						switch (FEAT_TRAINS,SELF,switch_cargo_subtype_uicx_d, cargo_subtype) {
							0: 	return string(STR_LIVERY_build_year);
							1:  return string(STR_LIVERY_current_year);
							2:	return string(STR_LIVERY_GREEN); 
							3:	return string(STR_LIVERY_DB_OB);
							4:	return string(STR_LIVERY_DB_GRAY);
							5:  return string(STR_LIVERY_SILBERLING_MINT);
							6:  return string(STR_LIVERY_DB_TRAFFICRED); 
						   	return CB_RESULT_NO_TEXT;
							}
							
						//Reihenfolge	
						switch(FEAT_TRAINS, SELF, switch_uicx_d_graphics, position_in_articulated_veh  % 3) {
							0:     spriteset_empty;
							1:     return switch_sprite_choose_uicx_d;
							2:     spriteset_empty;
							}															
					

						switch(FEAT_TRAINS, SELF, switch_length_uicx_d, position_in_articulated_veh % 3) {
						// Size of the first part
						0: 4;
						1: 7;
						2: 4;
						}


						switch(FEAT_TRAINS, SELF, switch_speed_uicx_d, current_year) {
						1950 .. 1969 : 140 ;
						1970 .. 2050 : 160 ;
						}


						//set number of articulated parts
						switch (FEAT_TRAINS, SELF, switch_articulated_uicx_d, extra_callback_info1) {
						1: return ITEM_UICX_D;
						2: return ITEM_UICX_D;															//use same vehicle for all parts
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS; 									//stop adding vehicle parts
						}
	
	
