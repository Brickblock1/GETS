spriteset (spriteset_UICX_BDMS_purchase, "gfx\UICX/UICX_BDMS_gruen_1x_8bpp.png")
					{template_purchase_158 (0, 0)}			
	
		
//Switches


						switch (FEAT_TRAINS,SELF,switch_build_year_uicx_bdms,build_year) {
						1950 .. 1973 : spritegroup_UICX_BDMS_gruen;
						1974 .. 1987 : spritegroup_UICX_BDMS_ozeanblau;
						1988 .. 1998 : spritegroup_UICX_BDMS_ic_alt;
						1999 .. 2050 : spritegroup_UICX_BDMS_verkehrsrot;
						spritegroup_UICX_BDMS_ozeanblau;
						}		

						
						switch (FEAT_TRAINS,SELF,switch_current_year_uicx_bdms,current_year) {
						1950 .. 1973 : spritegroup_UICX_BDMS_gruen;
						1974 .. 1987 : spritegroup_UICX_BDMS_ozeanblau;
						1988 .. 1998 : spritegroup_UICX_BDMS_ic_alt;
						1999 .. 2050 : spritegroup_UICX_BDMS_verkehrsrot;
						spritegroup_UICX_BDMS_ozeanblau;
						}	
						
						
						//Manuelle Auswahl Spriteset
						switch (FEAT_TRAINS,SELF, switch_sprite_choose_uicx_bdms, cargo_subtype) {
							0:  switch_build_year_uicx_bdms;
							1:  switch_current_year_uicx_bdms;
							2:	spritegroup_UICX_BDMS_gruen; 
							3:	spritegroup_UICX_BDMS_ozeanblau;
							4:	spritegroup_UICX_BDMS_ic_alt;
							5:  spritegroup_UICX_BDMS_verkehrsrot;
							switch_build_year_uicx_bdms;
							} 	
							
						//Auswahl Menu
						switch (FEAT_TRAINS,SELF,switch_cargo_subtype_uicx_bdms, cargo_subtype) {
							0: 	return string(STR_LIVERY_build_year);
							1:  return string(STR_LIVERY_current_year);
							2:	return string(STR_LIVERY_GREEN); 
							3:	return string(STR_LIVERY_DB_OB);
							4:	return string(STR_LIVERY_DB_IC_OLD);
							5:  return string(STR_LIVERY_DB_TRAFFICRED); 
						   	return CB_RESULT_NO_TEXT;
							}
							
						//Reihenfolge	
						switch(FEAT_TRAINS, SELF, switch_uicx_bdms_graphics, position_in_articulated_veh  % 3) {
							0:     spriteset_empty;
							1:     return switch_sprite_choose_uicx_bdms;
							2:     spriteset_empty;
							}															
					

						switch(FEAT_TRAINS, SELF, switch_length_uicx_bdms, position_in_articulated_veh % 3) {
						// Size of the first part
						0: 4;
						1: 7;
						2: 4;
						}


						switch(FEAT_TRAINS, SELF, switch_speed_uicx_bdms, current_year) {
						1950 .. 1969 : 140 ;
						1970 .. 2050 : 160 ;
						}


						//set number of articulated parts
						switch (FEAT_TRAINS, SELF, switch_articulated_uicx_bdms, extra_callback_info1) {
						1: return ITEM_UICX_BDMS;
						2: return ITEM_UICX_BDMS;															//use same vehicle for all parts
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS; 									//stop adding vehicle parts
						}
	
	
