							1900 .. 1974 : random_choose_DVT_n_wagen_Hase;
							1975 .. 2050 : random_choose_DVT_n_wagen_Hase_OB;
							random_choose_DVT_n_wagen_Hase_OB;}						
							
					switch (FEAT_TRAINS, SELF, switch_current_year_DVT_n_wagen_Hase, current_year){
							1900 .. 1974 : random_choose_DVT_n_wagen_Hase;
							1975 .. 2050 : random_choose_DVT_n_wagen_Hase_OB;
							random_choose_DVT_n_wagen_Hase_OB;}

					switch (FEAT_TRAINS,SELF, switch_sprite_choose_DVT_n_wagen_Hase,cargo_subtype){
							0:  return switch_build_year_DVT_n_wagen_Hase;
							1:  return switch_current_year_DVT_n_wagen_Hase;
							2:	random_choose_DVT_n_wagen_Hase; 
							3:	random_choose_DVT_n_wagen_Hase_OB;
							switch_build_year_DVT_n_wagen_Hase;}	
							
					switch (FEAT_TRAINS,SELF,switch_cargo_subtype_DVT_n_wagen_Hase,cargo_subtype){
							0:  return string(STR_LIVERY_build_year);
							1:  return string(STR_LIVERY_current_year);
							2:	return string(STR_LIVERY_SILBERLING_BLK); 
							3:	return string(STR_LIVERY_SILBERLING_OB); 										
							return CB_RESULT_NO_TEXT;}
							
					switch(FEAT_TRAINS, SELF, switch_DVT_n_wagen_Hase_graphics, position_in_vehid_chain  % 3) {
							0:     spriteset_empty;
							1:     return switch_sprite_choose_DVT_n_wagen_Hase;
							2:     spriteset_empty;}																									
					
					switch(FEAT_TRAINS, SELF, switch_length_DVT_n_wagen_Hase, position_in_vehid_chain % 3) {
							0: 4;
							1: 7;
							2: 4;}							

					switch (FEAT_TRAINS, SELF, switch_articulated_DVT_n_wagen_Hase_MAIL, extra_callback_info1) {
							1: return ITEM_DVT_n_wagen_Hase_MAIL;
							2: return ITEM_DVT_n_wagen_Hase_MAILPART;
							return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}
							
					switch (FEAT_TRAINS, SELF, switch_articulated_DVT_n_wagen_Hase, extra_callback_info1) {
							1: return ITEM_DVT_n_wagen_Hase;
							2: return ITEM_DVT_n_wagen_Hase;
							return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}												

