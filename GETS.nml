							1: spriteset_BR_24_purchase_anim;}						
															
						switch (FEAT_TRAINS, SELF, switch_animation_BR_24, motion_counter % 4) {
							0: spriteset_BR_24_anim1;
							1: spriteset_BR_24_anim2;
							2: spriteset_BR_24_anim3;
							3: spriteset_BR_24_anim4;}									

						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_24_DRG, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_24_DRG;
							1: return switch_animation_BR_24;}
						
						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_24_DB_Wagner, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_24_DB_Wagner;
							1: return switch_animation_BR_24;}						
						
						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_24_DB_Witte, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_BR_24_DB_Witte;
							1: return switch_animation_BR_24;}							

						switch (FEAT_TRAINS, SELF, random_choose_BR_24_DB, switch_alternative_var_random_bits(1, 1)) {
							1: switch_spritestack_BR_24_DB_Wagner;
								switch_spritestack_BR_24_DB_Witte;}	

						switch (FEAT_TRAINS,SELF, switch_BR_24_purchase,current_year){
							1900 .. 1945: switch_spritestack_BR_24_purchase1;
							1946 .. 1990: switch_spritestack_BR_24_purchase2;
										switch_spritestack_BR_24_purchase2;}
								
						switch (FEAT_TRAINS,SELF, switch_BR_24_build_year,build_year){
							1900 .. 1945: switch_spritestack_BR_24_DRG;
							1946 .. 1990: random_choose_BR_24_DB;
									random_choose_BR_24_DB;}								

						switch (FEAT_TRAINS,SELF, switch_BR_24_tender_build_year,build_year){
							1900 .. 1945: spriteset_BR_24_Tender_DRG;
							1946 .. 1990: spriteset_BR_24_Tender_DB;
									spriteset_BR_24_Tender_DB;}

						switch (FEAT_TRAINS,SELF, switch_BR_24_current_year,current_year){
							1900 .. 1945: switch_spritestack_BR_24_DRG;
							1946 .. 1990: random_choose_BR_24_DB;
									random_choose_BR_24_DB;}														

						switch (FEAT_TRAINS,SELF, switch_BR_24_tender_current_year,current_year){
							1900 .. 1945: spriteset_BR_24_Tender_DRG;
							1946 .. 1990: spriteset_BR_24_Tender_DB;
									spriteset_BR_24_Tender_DB;}

						switch (FEAT_TRAINS,SELF, switch_sprite_choose_BR_24,cargo_subtype){
							0:	switch_BR_24_build_year;
							1:  switch_BR_24_current_year;
							2:	switch_spritestack_BR_24_DRG;
							3:  switch_spritestack_BR_24_DB_Wagner;
							4:  switch_spritestack_BR_24_DB_Witte;
							switch_BR_24_build_year;} 		

						switch (FEAT_TRAINS,SELF, switch_sprite_choose_BR_24_tender,cargo_subtype){
							0:  switch_BR_24_tender_build_year;
							1:  switch_BR_24_tender_current_year;
							2:	spriteset_BR_24_Tender_DRG;
							3:  spriteset_BR_24_Tender_DB;
							4:  spriteset_BR_24_Tender_DB;
							switch_BR_24_tender_build_year;} 
							
						switch (FEAT_TRAINS,SELF,switch_cargo_subtype_BR_24 ,cargo_subtype){
							0:	return string(STR_LIVERY_build_year); 
							1:  return string(STR_LIVERY_current_year);
							2:	return string(STR_LIVERY_DRG_WAGNER); 
							3:	return string(STR_LIVERY_DB_WAGNER);
							4:  return string(STR_LIVERY_DB_WITTE);
							return CB_RESULT_NO_TEXT;}
							
						switch(FEAT_TRAINS, SELF, switch_BR_24_graphics, position_in_vehid_chain % 2) {
							0:	   return switch_sprite_choose_BR_24;
							1:     return switch_sprite_choose_BR_24_tender;}															

						switch(FEAT_TRAINS, SELF, switch_length_BR_24, position_in_vehid_chain % 2) {
							0: 7;
							1: 4;}
						
						switch(FEAT_TRAINS, SELF, switch_FX_BR_24, position_in_vehid_chain % 2) {
							0: return visual_effect_and_powered(VISUAL_EFFECT_STEAM, -2, DISABLE_WAGON_POWER);
							return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);}						

						switch (FEAT_TRAINS, SELF, switch_articulated_BR_24, extra_callback_info1) {
							1: return ITEM_BR_24;
							return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}
						
						switch (FEAT_TRAINS, PARENT, switch_running_cost_BR_24, 1) {
							return 1175;}

